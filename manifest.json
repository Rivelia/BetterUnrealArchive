{
	"name": "Better Unreal Archives",
	"description": "Improves user experience on unrealarchive.org",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://unrealarchive.org/*/index.htm*"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"96": "icon96.png",
		"128": "icon128.png"
	}
}
